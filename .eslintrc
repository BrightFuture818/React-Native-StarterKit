// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
  	"classes": true,
  },
  rules: {
  	'react/jsx-filename-extension': ['error', { extensions: ['.js', '.jsx'] }],
  	'global-require': 'off',
    'no-console': 'off',
	},
}
